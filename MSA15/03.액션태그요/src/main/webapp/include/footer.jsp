<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!-- JSP 페이지 설정: HTML로 출력하며 UTF-8로 인코딩 -->

<!DOCTYPE html>
<!-- HTML5 문서 타입 선언 -->

<html>
<!-- HTML 문서 시작 -->

<head>
<!-- head 영역 시작 -->

<meta charset="UTF-8">
<!-- 브라우저가 UTF-8로 렌더링하도록 설정 -->

<title>Insert title here</title>
<!-- 브라우저 탭 제목 -->

</head>
<!-- head 영역 끝 -->

<body>
<!-- body 시작 -->

<footer>
<!-- footer 영역 시작 -->

<p>푸터 영역</p>
<!-- 단순 문구 출력 -->

<p>Copyright All rights reserved.</p>
<!-- 저작권 문구 출력 -->

</footer>
<!-- footer 끝 -->

</body>
<!-- body 종료 -->

</html>
<!-- HTML 문서 종료 -->
